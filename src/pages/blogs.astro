---
import Layout from "../layouts/layout.astro";
import Button from "../components/Button.astro";
const response = await fetch('https://dev.to/api/articles?username=davidadediji');
const posts = await response.json();
---

<Layout title="David Adediji | Blogs">
    <h1 class="text-5xl">Blogs</h1>
    <Button navText="Home"/>
    {
        posts.map(post=><h1>{post.title}</h1>)
    }
</Layout>